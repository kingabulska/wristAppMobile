<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/signInFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="pl.kinga.wristapp.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
        <action
            android:id="@+id/action_splashFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/ovalFragment"
        android:name="pl.kinga.wristapp.ui.LeftOvalFragment"
        android:label="OvalFragment" >
        <action
            android:id="@+id/action_ovalFragment_to_rightStartFragment"
            app:destination="@id/rightStartFragment" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment"
        android:name="pl.kinga.wristapp.ui.InfoFragment"
        android:label="InfoFragment" >
        <action
            android:id="@+id/action_infoFragment_to_info2Fragment"
            app:destination="@id/info2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/info2Fragment"
        android:name="pl.kinga.wristapp.ui.Info2Fragment"
        android:label="Info2Fragment" >
        <action
            android:id="@+id/action_info2Fragment_to_info3Fragment"
            app:destination="@id/info3Fragment" />
    </fragment>
    <fragment
        android:id="@+id/info3Fragment"
        android:name="pl.kinga.wristapp.ui.Info3Fragment"
        android:label="Info3Fragment" >
        <action
            android:id="@+id/action_info3Fragment_to_info4Fragment"
            app:destination="@id/info4Fragment" />
    </fragment>
    <fragment
        android:id="@+id/info4Fragment"
        android:name="pl.kinga.wristapp.ui.Info4Fragment"
        android:label="Info4Fragment" >
        <action
            android:id="@+id/action_info4Fragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
    <fragment
        android:id="@+id/endFragment"
        android:name="pl.kinga.wristapp.ui.EndFragment"
        android:label="EndFragment" >
        <action
            android:id="@+id/action_endFragment_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
    <fragment
        android:id="@+id/startFragment"
        android:name="pl.kinga.wristapp.ui.LeftStartFragment"
        android:label="StartFragment" >
        <action
            android:id="@+id/action_startFragment_to_ovalFragment"
            app:destination="@id/ovalFragment" />
    </fragment>
    <fragment
        android:id="@+id/rightOvalFragment"
        android:name="pl.kinga.wristapp.ui.RightOvalFragment"
        android:label="RightOvalFragment" >
        <action
            android:id="@+id/action_rightOvalFragment_to_endFragment"
            app:destination="@id/endFragment" />
    </fragment>
    <fragment
        android:id="@+id/rightStartFragment"
        android:name="pl.kinga.wristapp.ui.RightStartFragment"
        android:label="RightStartFragment" >
        <action
            android:id="@+id/action_rightStartFragment_to_rightOvalFragment"
            app:destination="@id/rightOvalFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="pl.kinga.wristapp.ui.SignInFragment"
        android:label="SignInFragment" >
        <action
            android:id="@+id/action_signInFragment_to_infoFragment"
            app:destination="@id/infoFragment" />
    </fragment>
</navigation>